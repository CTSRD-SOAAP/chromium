build obj/ipc/ipc_tests.actions_depends.stamp: stamp obj/ipc/libipc.a $
    obj/ipc/libtest_support_ipc.a obj/base/libbase.a $
    obj/base/libbase_i18n.a obj/base/librun_all_unittests.a $
    obj/base/libtest_support_base.a obj/testing/libgtest.a $
    obj/base/libbase_static.a $
    obj/base/allocator/liballocator_extension_thunks.a $
    obj/third_party/modp_b64/libmodp_b64.a $
    obj/base/third_party/dynamic_annotations/libdynamic_annotations.a $
    obj/base/libsymbolize.a obj/base/libxdg_mime.a $
    obj/third_party/libevent/libevent.a obj/third_party/icu/libicui18n.a $
    obj/third_party/icu/libicuuc.a obj/third_party/icu/libicudata.a $
    obj/testing/libgmock.a

defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DUNIT_TEST -DGTEST_HAS_RTTI=0 -D__STDC_CONSTANT_MACROS $
    -D__STDC_FORMAT_MACROS -DNDEBUG -DOFFICIAL_BUILD -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../.. -I../../testing/gtest/include
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics $
    -I/usr/local/include/gtk-2.0 -I/usr/local/include/gio-unix-2.0/ $
    -I/usr/local/include/atk-1.0 -I/usr/local/include/cairo $
    -I/usr/local/include/pixman-1 -D_THREAD_SAFE $
    -I/usr/local/include/gdk-pixbuf-2.0 -I/usr/local/include/libpng15 $
    -I/usr/local/include/pango-1.0 -I/usr/local/include/freetype2 $
    -I/usr/local/include/harfbuzz -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -D_THREAD_SAFE -I/usr/local/include -O2 $
    -fdata-sections -ffunction-sections
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -Wsign-compare $
    -std=gnu++11

build obj/ipc/ipc_tests.file_descriptor_set_posix_unittest.o: cxx $
    ../../ipc/file_descriptor_set_posix_unittest.cc
build obj/ipc/ipc_tests.ipc_channel_posix_unittest.o: cxx $
    ../../ipc/ipc_channel_posix_unittest.cc
build obj/ipc/ipc_tests.ipc_channel_unittest.o: cxx $
    ../../ipc/ipc_channel_unittest.cc
build obj/ipc/ipc_tests.ipc_fuzzing_tests.o: cxx $
    ../../ipc/ipc_fuzzing_tests.cc
build obj/ipc/ipc_tests.ipc_message_unittest.o: cxx $
    ../../ipc/ipc_message_unittest.cc
build obj/ipc/ipc_tests.ipc_message_utils_unittest.o: cxx $
    ../../ipc/ipc_message_utils_unittest.cc
build obj/ipc/ipc_tests.ipc_send_fds_test.o: cxx $
    ../../ipc/ipc_send_fds_test.cc
build obj/ipc/ipc_tests.ipc_sync_channel_unittest.o: cxx $
    ../../ipc/ipc_sync_channel_unittest.cc
build obj/ipc/ipc_tests.ipc_sync_message_unittest.o: cxx $
    ../../ipc/ipc_sync_message_unittest.cc
build obj/ipc/ipc_tests.ipc_test_base.o: cxx ../../ipc/ipc_test_base.cc
build obj/ipc/ipc_tests.sync_socket_unittest.o: cxx $
    ../../ipc/sync_socket_unittest.cc
build obj/ipc/ipc_tests.unix_domain_socket_util_unittest.o: cxx $
    ../../ipc/unix_domain_socket_util_unittest.cc

ldflags = -Wl,-z,now -Wl,-z,relro -pthread -Wl,-z,noexecstack -fPIC $
    -Wl,--no-keep-memory -L/usr/local/lib -Wl,-O1 -Wl,--as-needed
libs =  -L/usr/local/lib -lexecinfo -lgmodule-2.0 -lgobject-2.0 $
    -lgthread-2.0 -lglib-2.0 -lintl -lXtst -lgtk-x11-2.0 -lgdk-x11-2.0 $
    -lpangocairo-1.0 -lXext -lXrender -lXinerama -lXi -lXrandr -lXcursor $
    -lXcomposite -lXdamage -lXfixes -lX11 -latk-1.0 -lcairo $
    -lgdk_pixbuf-2.0 -lgio-2.0 -lpangoft2-1.0 -lpango-1.0 -lfreetype $
    -lfontconfig
build ipc_tests: link $
    obj/ipc/ipc_tests.file_descriptor_set_posix_unittest.o $
    obj/ipc/ipc_tests.ipc_channel_posix_unittest.o $
    obj/ipc/ipc_tests.ipc_channel_unittest.o $
    obj/ipc/ipc_tests.ipc_fuzzing_tests.o $
    obj/ipc/ipc_tests.ipc_message_unittest.o $
    obj/ipc/ipc_tests.ipc_message_utils_unittest.o $
    obj/ipc/ipc_tests.ipc_send_fds_test.o $
    obj/ipc/ipc_tests.ipc_sync_channel_unittest.o $
    obj/ipc/ipc_tests.ipc_sync_message_unittest.o $
    obj/ipc/ipc_tests.ipc_test_base.o $
    obj/ipc/ipc_tests.sync_socket_unittest.o $
    obj/ipc/ipc_tests.unix_domain_socket_util_unittest.o obj/ipc/libipc.a $
    obj/third_party/icu/libicuuc.a $
    obj/base/allocator/liballocator_extension_thunks.a $
    obj/third_party/icu/libicui18n.a obj/testing/libgtest.a $
    obj/base/libbase_static.a obj/third_party/modp_b64/libmodp_b64.a $
    obj/base/libtest_support_base.a obj/base/libbase_i18n.a $
    obj/base/libbase.a obj/base/librun_all_unittests.a $
    obj/base/libxdg_mime.a obj/base/libsymbolize.a $
    obj/base/third_party/dynamic_annotations/libdynamic_annotations.a $
    obj/ipc/libtest_support_ipc.a obj/third_party/libevent/libevent.a $
    obj/third_party/icu/libicudata.a obj/testing/libgmock.a
  postbuilds = 

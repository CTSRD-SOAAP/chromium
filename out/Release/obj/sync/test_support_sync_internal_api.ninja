build obj/sync/test_support_sync_internal_api.actions_depends.stamp: stamp $
    obj/sync/sync.actions_depends.stamp obj/sync/libsync_proto.a

build obj/sync/test_support_sync_internal_api.compile_depends.stamp: stamp $
    obj/sync/sync.compile_depends.stamp $
    obj/sync/sync_proto.actions_rules_copies.stamp

defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DSYNC_TEST -DUNIT_TEST -DGTEST_HAS_RTTI=0 -DFEATURE_ENABLE_SSL $
    -DFEATURE_ENABLE_VOICEMAIL -DEXPAT_RELATIVE_PATH -DGTEST_RELATIVE_PATH $
    -DJSONCPP_RELATIVE_PATH -DNO_MAIN_THREAD_WRAPPING -DNO_SOUND_SYSTEM $
    -DPOSIX -DBSD -DFREEBSD -DPROTOBUF_USE_DLLS -DGOOGLE_PROTOBUF_NO_RTTI $
    -DGOOGLE_PROTOBUF_NO_STATIC_INITIALIZER -D__STDC_CONSTANT_MACROS $
    -D__STDC_FORMAT_MACROS -DNDEBUG -DOFFICIAL_BUILD -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../.. -I../../testing/gtest/include $
    -I../../third_party/libjingle/overrides $
    -I../../third_party/libjingle/source -I../../third_party $
    -I../../third_party/webrtc $
    -I../../third_party/jsoncpp/overrides/include $
    -I../../third_party/jsoncpp/source/include $
    -I../../third_party/cacheinvalidation/overrides $
    -I../../third_party/cacheinvalidation/src -Igen/protoc_out $
    -I../../third_party/protobuf -I../../third_party/protobuf/src $
    -I../../testing/gmock/include
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics $
    -I/usr/local/include/gtk-2.0 -I/usr/local/include/gio-unix-2.0/ $
    -I/usr/local/include/atk-1.0 -I/usr/local/include/cairo $
    -I/usr/local/include/pixman-1 -D_THREAD_SAFE $
    -I/usr/local/include/gdk-pixbuf-2.0 -I/usr/local/include/libpng15 $
    -I/usr/local/include/pango-1.0 -I/usr/local/include/freetype2 $
    -I/usr/local/include/harfbuzz -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -D_THREAD_SAFE -I/usr/local/include $
    -Wexit-time-destructors -O2 -fdata-sections -ffunction-sections
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wsign-compare -std=gnu++11

build $
    obj/sync/internal_api/public/base/test_support_sync_internal_api.invalidation_test_util.o: $
    cxx ../../sync/internal_api/public/base/invalidation_test_util.cc || $
    obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/public/base/test_support_sync_internal_api.object_id_invalidation_map_test_util.o: $
    cxx $
    ../../sync/internal_api/public/base/object_id_invalidation_map_test_util.cc $
    || obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.fake_sync_manager.o: $
    cxx ../../sync/internal_api/test/fake_sync_manager.cc || $
    obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.sync_manager_factory_for_profile_sync_test.o: $
    cxx $
    ../../sync/internal_api/test/sync_manager_factory_for_profile_sync_test.cc $
    || obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.sync_manager_for_profile_sync_test.o: $
    cxx ../../sync/internal_api/test/sync_manager_for_profile_sync_test.cc $
    || obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_entry_factory.o: $
    cxx ../../sync/internal_api/test/test_entry_factory.cc || $
    obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_internal_components_factory.o: $
    cxx ../../sync/internal_api/test/test_internal_components_factory.cc || $
    obj/sync/test_support_sync_internal_api.compile_depends.stamp
build $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_user_share.o: $
    cxx ../../sync/internal_api/test/test_user_share.cc || $
    obj/sync/test_support_sync_internal_api.compile_depends.stamp

build obj/sync/libtest_support_sync_internal_api.a: alink_thin $
    obj/sync/internal_api/public/base/test_support_sync_internal_api.invalidation_test_util.o $
    obj/sync/internal_api/public/base/test_support_sync_internal_api.object_id_invalidation_map_test_util.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.fake_sync_manager.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.sync_manager_factory_for_profile_sync_test.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.sync_manager_for_profile_sync_test.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_entry_factory.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_internal_components_factory.o $
    obj/sync/internal_api/test/test_support_sync_internal_api.test_user_share.o $
    || obj/sync/test_support_sync_internal_api.actions_depends.stamp

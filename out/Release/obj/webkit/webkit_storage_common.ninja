defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DWEBKIT_STORAGE_COMMON_IMPLEMENTATION -D__STDC_CONSTANT_MACROS $
    -D__STDC_FORMAT_MACROS -DNDEBUG -DOFFICIAL_BUILD -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -D_FORTIFY_SOURCE=2
includes = -I../..
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics $
    -I/usr/local/include/gtk-2.0 -I/usr/local/include/gio-unix-2.0/ $
    -I/usr/local/include/atk-1.0 -I/usr/local/include/cairo $
    -I/usr/local/include/pixman-1 -D_THREAD_SAFE $
    -I/usr/local/include/gdk-pixbuf-2.0 -I/usr/local/include/libpng15 $
    -I/usr/local/include/pango-1.0 -I/usr/local/include/freetype2 $
    -I/usr/local/include/harfbuzz -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -D_THREAD_SAFE -I/usr/local/include $
    -Wexit-time-destructors -O2 -fdata-sections -ffunction-sections
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -Wsign-compare -std=gnu++11

build $
    obj/webkit/common/appcache/webkit_storage_common.appcache_interfaces.o: $
    cxx ../../webkit/common/appcache/appcache_interfaces.cc
build obj/webkit/common/blob/webkit_storage_common.blob_data.o: cxx $
    ../../webkit/common/blob/blob_data.cc
build obj/webkit/common/blob/webkit_storage_common.scoped_file.o: cxx $
    ../../webkit/common/blob/scoped_file.cc
build $
    obj/webkit/common/blob/webkit_storage_common.shareable_file_reference.o: $
    cxx ../../webkit/common/blob/shareable_file_reference.cc
build $
    obj/webkit/common/database/webkit_storage_common.database_connections.o: $
    cxx ../../webkit/common/database/database_connections.cc
build $
    obj/webkit/common/database/webkit_storage_common.database_identifier.o: $
    cxx ../../webkit/common/database/database_identifier.cc
build obj/webkit/common/fileapi/webkit_storage_common.directory_entry.o: $
    cxx ../../webkit/common/fileapi/directory_entry.cc
build obj/webkit/common/fileapi/webkit_storage_common.file_system_info.o: $
    cxx ../../webkit/common/fileapi/file_system_info.cc
build obj/webkit/common/fileapi/webkit_storage_common.file_system_util.o: $
    cxx ../../webkit/common/fileapi/file_system_util.cc
build obj/webkit/common/quota/webkit_storage_common.quota_status_code.o: $
    cxx ../../webkit/common/quota/quota_status_code.cc

build obj/webkit/libwebkit_storage_common.a: alink_thin $
    obj/webkit/common/appcache/webkit_storage_common.appcache_interfaces.o $
    obj/webkit/common/blob/webkit_storage_common.blob_data.o $
    obj/webkit/common/blob/webkit_storage_common.scoped_file.o $
    obj/webkit/common/blob/webkit_storage_common.shareable_file_reference.o $
    obj/webkit/common/database/webkit_storage_common.database_connections.o $
    obj/webkit/common/database/webkit_storage_common.database_identifier.o $
    obj/webkit/common/fileapi/webkit_storage_common.directory_entry.o $
    obj/webkit/common/fileapi/webkit_storage_common.file_system_info.o $
    obj/webkit/common/fileapi/webkit_storage_common.file_system_util.o $
    obj/webkit/common/quota/webkit_storage_common.quota_status_code.o

build obj/sync/test_support_sync_notifier.actions_depends.stamp: stamp $
    obj/third_party/cacheinvalidation/libcacheinvalidation_proto_cpp.a $
    obj/sync/sync.actions_depends.stamp obj/sync/libsync_proto.a

build obj/sync/test_support_sync_notifier.compile_depends.stamp: stamp $
    obj/third_party/cacheinvalidation/cacheinvalidation_proto_cpp.actions_rules_copies.stamp $
    obj/sync/sync.compile_depends.stamp $
    obj/sync/sync_proto.actions_rules_copies.stamp

defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DSYNC_TEST -DUNIT_TEST -DGTEST_HAS_RTTI=0 -DPROTOBUF_USE_DLLS $
    -DGOOGLE_PROTOBUF_NO_RTTI -DGOOGLE_PROTOBUF_NO_STATIC_INITIALIZER $
    -DFEATURE_ENABLE_SSL -DFEATURE_ENABLE_VOICEMAIL -DEXPAT_RELATIVE_PATH $
    -DGTEST_RELATIVE_PATH -DJSONCPP_RELATIVE_PATH -DNO_MAIN_THREAD_WRAPPING $
    -DNO_SOUND_SYSTEM -DPOSIX -DBSD -DFREEBSD -D__STDC_CONSTANT_MACROS $
    -D__STDC_FORMAT_MACROS -DDYNAMIC_ANNOTATIONS_ENABLED=1 $
    -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../.. -I../../testing/gmock/include $
    -I../../testing/gtest/include $
    -I../../third_party/cacheinvalidation/google/cacheinvalidation $
    -Igen/protoc_out -I../../third_party/protobuf $
    -I../../third_party/protobuf/src $
    -I../../third_party/libjingle/overrides $
    -I../../third_party/libjingle/source -I../../third_party $
    -I../../third_party/webrtc $
    -I../../third_party/jsoncpp/overrides/include $
    -I../../third_party/jsoncpp/source/include $
    -I../../third_party/cacheinvalidation/overrides $
    -I../../third_party/cacheinvalidation/src
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics $
    -I/usr/local/include/gtk-2.0 -I/usr/local/include/gio-unix-2.0/ $
    -I/usr/local/include/atk-1.0 -I/usr/local/include/cairo $
    -I/usr/local/include/pixman-1 -D_THREAD_SAFE $
    -I/usr/local/include/gdk-pixbuf-2.0 -I/usr/local/include/libpng15 $
    -I/usr/local/include/pango-1.0 -I/usr/local/include/freetype2 $
    -I/usr/local/include/harfbuzz -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -D_THREAD_SAFE -I/usr/local/include -O0 -g
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -Wsign-compare $
    -std=gnu++11

build $
    obj/sync/notifier/test_support_sync_notifier.fake_invalidation_state_tracker.o: $
    cxx ../../sync/notifier/fake_invalidation_state_tracker.cc || $
    obj/sync/test_support_sync_notifier.compile_depends.stamp
build obj/sync/notifier/test_support_sync_notifier.fake_invalidator.o: cxx $
    ../../sync/notifier/fake_invalidator.cc || $
    obj/sync/test_support_sync_notifier.compile_depends.stamp
build $
    obj/sync/notifier/test_support_sync_notifier.fake_invalidation_handler.o: $
    cxx ../../sync/notifier/fake_invalidation_handler.cc || $
    obj/sync/test_support_sync_notifier.compile_depends.stamp
build $
    obj/sync/notifier/test_support_sync_notifier.invalidator_test_template.o: $
    cxx ../../sync/notifier/invalidator_test_template.cc || $
    obj/sync/test_support_sync_notifier.compile_depends.stamp
build $
    obj/sync/notifier/test_support_sync_notifier.object_id_invalidation_map_test_util.o: $
    cxx ../../sync/notifier/object_id_invalidation_map_test_util.cc || $
    obj/sync/test_support_sync_notifier.compile_depends.stamp

build obj/sync/libtest_support_sync_notifier.a: alink_thin $
    obj/sync/notifier/test_support_sync_notifier.fake_invalidation_state_tracker.o $
    obj/sync/notifier/test_support_sync_notifier.fake_invalidator.o $
    obj/sync/notifier/test_support_sync_notifier.fake_invalidation_handler.o $
    obj/sync/notifier/test_support_sync_notifier.invalidator_test_template.o $
    obj/sync/notifier/test_support_sync_notifier.object_id_invalidation_map_test_util.o $
    || obj/sync/test_support_sync_notifier.actions_depends.stamp

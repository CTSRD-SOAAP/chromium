@hex:
  # Legacy instructions which are invalid in 64-bit mode
  # nops (90) are used to reconsile disassembler
  # aaa
  37
  # aad
  D5 0A
@dis:
     0:	37                                           	(bad)  
     1:	d5                                           	(bad)  
     2:	0a                                           	.byte 0xa
@rdfa_output:
  0: [0] unrecognized instruction
  1: [1] unrecognized instruction
  return code: 1

defines = -DANGLE_DX11 -DANGLE_DISABLE_TRACE $
    -DANGLE_COMPILE_OPTIMIZATION_LEVEL=D3DCOMPILE_OPTIMIZATION_LEVEL1 $
    '-DANGLE_PRELOADED_D3DCOMPILER_MODULE_NAMES={ $
    TEXT("d3dcompiler_46.dll"), TEXT("d3dcompiler_43.dll") }' $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 -DUSE_NSS=1 $
    -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN -DGTK_DISABLE_SINGLE_INCLUDES=1 $
    -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 -DENABLE_CONFIGURATION_POLICY $
    -DENABLE_INPUT_SPEECH -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 $
    -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DCOMPILER_IMPLEMENTATION -DNDEBUG -DOFFICIAL_BUILD -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0
includes = -I../../third_party/angle_dx11/src $
    -I../../third_party/angle_dx11/include
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics -Wno-format -O2 $
    -fdata-sections -ffunction-sections
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -std=gnu++11 $
    -Wno-deprecated

build $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.CodeGenGLSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/CodeGenGLSL.cpp
build obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputESSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/OutputESSL.cpp
build $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputGLSLBase.o: $
    cxx ../../third_party/angle_dx11/src/compiler/OutputGLSLBase.cpp
build obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputGLSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/OutputGLSL.cpp
build obj/third_party/angle_dx11/src/compiler/translator_glsl.ShaderLang.o: $
    cxx ../../third_party/angle_dx11/src/compiler/ShaderLang.cpp
build $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.TranslatorESSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/TranslatorESSL.cpp
build $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.TranslatorGLSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/TranslatorGLSL.cpp
build $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.VersionGLSL.o: $
    cxx ../../third_party/angle_dx11/src/compiler/VersionGLSL.cpp

build obj/third_party/angle_dx11/src/libtranslator_glsl.a: alink_thin $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.CodeGenGLSL.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputESSL.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputGLSLBase.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.OutputGLSL.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.ShaderLang.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.TranslatorESSL.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.TranslatorGLSL.o $
    obj/third_party/angle_dx11/src/compiler/translator_glsl.VersionGLSL.o

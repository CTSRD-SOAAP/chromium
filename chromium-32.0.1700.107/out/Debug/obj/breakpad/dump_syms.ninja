defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DHAVE_A_OUT_H -DDYNAMIC_ANNOTATIONS_ENABLED=1 $
    -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../breakpad/src -I../..
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -std=gnu++11 -Wno-deprecated

build obj/breakpad/src/common/dwarf/dump_syms.bytereader.o: cxx $
    ../../breakpad/src/common/dwarf/bytereader.cc
build obj/breakpad/src/common/dump_syms.dwarf_cfi_to_module.o: cxx $
    ../../breakpad/src/common/dwarf_cfi_to_module.cc
build obj/breakpad/src/common/dump_syms.dwarf_cu_to_module.o: cxx $
    ../../breakpad/src/common/dwarf_cu_to_module.cc
build obj/breakpad/src/common/dwarf/dump_syms.dwarf2diehandler.o: cxx $
    ../../breakpad/src/common/dwarf/dwarf2diehandler.cc
build obj/breakpad/src/common/dwarf/dump_syms.dwarf2reader.o: cxx $
    ../../breakpad/src/common/dwarf/dwarf2reader.cc
build obj/breakpad/src/common/dump_syms.dwarf_line_to_module.o: cxx $
    ../../breakpad/src/common/dwarf_line_to_module.cc
build obj/breakpad/src/common/dump_syms.language.o: cxx $
    ../../breakpad/src/common/language.cc
build obj/breakpad/src/common/linux/dump_syms.dump_symbols.o: cxx $
    ../../breakpad/src/common/linux/dump_symbols.cc
build obj/breakpad/src/common/linux/dump_syms.elf_symbols_to_module.o: cxx $
    ../../breakpad/src/common/linux/elf_symbols_to_module.cc
build obj/breakpad/src/common/linux/dump_syms.elfutils.o: cxx $
    ../../breakpad/src/common/linux/elfutils.cc
build obj/breakpad/src/common/linux/dump_syms.file_id.o: cxx $
    ../../breakpad/src/common/linux/file_id.cc
build obj/breakpad/src/common/linux/dump_syms.linux_libc_support.o: cxx $
    ../../breakpad/src/common/linux/linux_libc_support.cc
build obj/breakpad/src/common/linux/dump_syms.memory_mapped_file.o: cxx $
    ../../breakpad/src/common/linux/memory_mapped_file.cc
build obj/breakpad/src/common/dump_syms.module.o: cxx $
    ../../breakpad/src/common/module.cc
build obj/breakpad/src/common/dump_syms.stabs_reader.o: cxx $
    ../../breakpad/src/common/stabs_reader.cc
build obj/breakpad/src/common/dump_syms.stabs_to_module.o: cxx $
    ../../breakpad/src/common/stabs_to_module.cc
build obj/breakpad/src/tools/linux/dump_syms/dump_syms.dump_syms.o: cxx $
    ../../breakpad/src/tools/linux/dump_syms/dump_syms.cc

ldflags = -Wl,-z,now -Wl,-z,relro -pthread -Wl,-z,noexecstack -fPIC $
    -Wl,--no-keep-memory
libs = 
build dump_syms: link obj/breakpad/src/common/dwarf/dump_syms.bytereader.o $
    obj/breakpad/src/common/dump_syms.dwarf_cfi_to_module.o $
    obj/breakpad/src/common/dump_syms.dwarf_cu_to_module.o $
    obj/breakpad/src/common/dwarf/dump_syms.dwarf2diehandler.o $
    obj/breakpad/src/common/dwarf/dump_syms.dwarf2reader.o $
    obj/breakpad/src/common/dump_syms.dwarf_line_to_module.o $
    obj/breakpad/src/common/dump_syms.language.o $
    obj/breakpad/src/common/linux/dump_syms.dump_symbols.o $
    obj/breakpad/src/common/linux/dump_syms.elf_symbols_to_module.o $
    obj/breakpad/src/common/linux/dump_syms.elfutils.o $
    obj/breakpad/src/common/linux/dump_syms.file_id.o $
    obj/breakpad/src/common/linux/dump_syms.linux_libc_support.o $
    obj/breakpad/src/common/linux/dump_syms.memory_mapped_file.o $
    obj/breakpad/src/common/dump_syms.module.o $
    obj/breakpad/src/common/dump_syms.stabs_reader.o $
    obj/breakpad/src/common/dump_syms.stabs_to_module.o $
    obj/breakpad/src/tools/linux/dump_syms/dump_syms.dump_syms.o
  ld = $ldxx

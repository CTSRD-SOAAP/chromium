rule $
    inspector_protocol_sources_target_generateInspectorProtocolBackendSources_3c8536faaa3ff0b0bc65df773e422ab7
  command = cd ../../third_party/WebKit/Source/core; python $
      inspector/CodeGeneratorInspector.py ../devtools/protocol.json $
      --output_h_dir ../../../../out/Release/gen/webkit --output_cpp_dir $
      ../../../../out/Release/gen/webcore
  description = ACTION Generating Inspector protocol backend sources from $
      protocol.json
  restat = 1

build gen/webcore/InspectorBackendDispatcher.cpp $
    gen/webkit/InspectorBackendDispatcher.h $
    gen/webcore/InspectorFrontend.cpp gen/webkit/InspectorFrontend.h $
    gen/webcore/InspectorTypeBuilder.cpp gen/webkit/InspectorTypeBuilder.h: $
    inspector_protocol_sources_target_generateInspectorProtocolBackendSources_3c8536faaa3ff0b0bc65df773e422ab7 $
    ../../third_party/WebKit/Source/core/inspector/CodeGeneratorInspector.py $
    ../../third_party/WebKit/Source/core/inspector/CodeGeneratorInspectorStrings.py $
    ../../third_party/WebKit/Source/devtools/protocol.json || $
    gen/webkit/InspectorProtocolVersion.h

build $
    obj/third_party/WebKit/Source/core/inspector_protocol_sources.actions_rules_copies.stamp: $
    stamp gen/webcore/InspectorBackendDispatcher.cpp $
    gen/webkit/InspectorBackendDispatcher.h $
    gen/webcore/InspectorFrontend.cpp gen/webkit/InspectorFrontend.h $
    gen/webcore/InspectorTypeBuilder.cpp gen/webkit/InspectorTypeBuilder.h


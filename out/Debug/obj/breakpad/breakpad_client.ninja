defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../breakpad/src -I../../breakpad/src/client $
    -I../../breakpad/src/third_party/linux/include -I../.. -I../../breakpad
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -std=gnu++11 -Wno-deprecated

build $
    obj/breakpad/src/client/linux/crash_generation/breakpad_client.crash_generation_client.o: $
    cxx $
    ../../breakpad/src/client/linux/crash_generation/crash_generation_client.cc
build $
    obj/breakpad/src/client/linux/handler/breakpad_client.exception_handler.o: $
    cxx ../../breakpad/src/client/linux/handler/exception_handler.cc
build $
    obj/breakpad/src/client/linux/handler/breakpad_client.minidump_descriptor.o: $
    cxx ../../breakpad/src/client/linux/handler/minidump_descriptor.cc
build obj/breakpad/src/client/linux/log/breakpad_client.log.o: cxx $
    ../../breakpad/src/client/linux/log/log.cc
build $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_core_dumper.o: $
    cxx ../../breakpad/src/client/linux/minidump_writer/linux_core_dumper.cc
build $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_dumper.o: $
    cxx ../../breakpad/src/client/linux/minidump_writer/linux_dumper.cc
build $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_ptrace_dumper.o: $
    cxx ../../breakpad/src/client/linux/minidump_writer/linux_ptrace_dumper.cc
build $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.minidump_writer.o: $
    cxx ../../breakpad/src/client/linux/minidump_writer/minidump_writer.cc
build obj/breakpad/src/client/breakpad_client.minidump_file_writer.o: cxx $
    ../../breakpad/src/client/minidump_file_writer.cc
build obj/breakpad/src/common/breakpad_client.convert_UTF.o: cc $
    ../../breakpad/src/common/convert_UTF.c
build obj/breakpad/src/common/linux/breakpad_client.elf_core_dump.o: cxx $
    ../../breakpad/src/common/linux/elf_core_dump.cc
build obj/breakpad/src/common/linux/breakpad_client.elfutils.o: cxx $
    ../../breakpad/src/common/linux/elfutils.cc
build obj/breakpad/src/common/linux/breakpad_client.file_id.o: cxx $
    ../../breakpad/src/common/linux/file_id.cc
build $
    obj/breakpad/src/common/linux/breakpad_client.google_crashdump_uploader.o: $
    cxx ../../breakpad/src/common/linux/google_crashdump_uploader.cc
build obj/breakpad/src/common/linux/breakpad_client.guid_creator.o: cxx $
    ../../breakpad/src/common/linux/guid_creator.cc
build obj/breakpad/src/common/linux/breakpad_client.libcurl_wrapper.o: cxx $
    ../../breakpad/src/common/linux/libcurl_wrapper.cc
build obj/breakpad/src/common/linux/breakpad_client.linux_libc_support.o: $
    cxx ../../breakpad/src/common/linux/linux_libc_support.cc
build obj/breakpad/src/common/linux/breakpad_client.memory_mapped_file.o: $
    cxx ../../breakpad/src/common/linux/memory_mapped_file.cc
build obj/breakpad/src/common/linux/breakpad_client.safe_readlink.o: cxx $
    ../../breakpad/src/common/linux/safe_readlink.cc
build obj/breakpad/src/common/breakpad_client.simple_string_dictionary.o: $
    cxx ../../breakpad/src/common/simple_string_dictionary.cc
build obj/breakpad/src/common/breakpad_client.string_conversion.o: cxx $
    ../../breakpad/src/common/string_conversion.cc

build obj/breakpad/libbreakpad_client.a: alink_thin $
    obj/breakpad/src/client/linux/crash_generation/breakpad_client.crash_generation_client.o $
    obj/breakpad/src/client/linux/handler/breakpad_client.exception_handler.o $
    obj/breakpad/src/client/linux/handler/breakpad_client.minidump_descriptor.o $
    obj/breakpad/src/client/linux/log/breakpad_client.log.o $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_core_dumper.o $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_dumper.o $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.linux_ptrace_dumper.o $
    obj/breakpad/src/client/linux/minidump_writer/breakpad_client.minidump_writer.o $
    obj/breakpad/src/client/breakpad_client.minidump_file_writer.o $
    obj/breakpad/src/common/breakpad_client.convert_UTF.o $
    obj/breakpad/src/common/linux/breakpad_client.elf_core_dump.o $
    obj/breakpad/src/common/linux/breakpad_client.elfutils.o $
    obj/breakpad/src/common/linux/breakpad_client.file_id.o $
    obj/breakpad/src/common/linux/breakpad_client.google_crashdump_uploader.o $
    obj/breakpad/src/common/linux/breakpad_client.guid_creator.o $
    obj/breakpad/src/common/linux/breakpad_client.libcurl_wrapper.o $
    obj/breakpad/src/common/linux/breakpad_client.linux_libc_support.o $
    obj/breakpad/src/common/linux/breakpad_client.memory_mapped_file.o $
    obj/breakpad/src/common/linux/breakpad_client.safe_readlink.o $
    obj/breakpad/src/common/breakpad_client.simple_string_dictionary.o $
    obj/breakpad/src/common/breakpad_client.string_conversion.o

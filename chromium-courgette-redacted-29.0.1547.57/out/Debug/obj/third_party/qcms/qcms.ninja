defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DSSE2_ENABLE -DDYNAMIC_ANNOTATIONS_ENABLED=1 $
    -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = 
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -std=gnu++11 $
    -Wno-deprecated

build obj/third_party/qcms/src/qcms.chain.o: cc $
    ../../third_party/qcms/src/chain.c
build obj/third_party/qcms/src/qcms.iccread.o: cc $
    ../../third_party/qcms/src/iccread.c
build obj/third_party/qcms/src/qcms.matrix.o: cc $
    ../../third_party/qcms/src/matrix.c
build obj/third_party/qcms/src/qcms.transform.o: cc $
    ../../third_party/qcms/src/transform.c
build obj/third_party/qcms/src/qcms.transform_util.o: cc $
    ../../third_party/qcms/src/transform_util.c
build obj/third_party/qcms/src/qcms.transform-sse1.o: cc $
    ../../third_party/qcms/src/transform-sse1.c
build obj/third_party/qcms/src/qcms.transform-sse2.o: cc $
    ../../third_party/qcms/src/transform-sse2.c

build obj/third_party/qcms/libqcms.a: alink_thin $
    obj/third_party/qcms/src/qcms.chain.o $
    obj/third_party/qcms/src/qcms.iccread.o $
    obj/third_party/qcms/src/qcms.matrix.o $
    obj/third_party/qcms/src/qcms.transform.o $
    obj/third_party/qcms/src/qcms.transform_util.o $
    obj/third_party/qcms/src/qcms.transform-sse1.o $
    obj/third_party/qcms/src/qcms.transform-sse2.o

defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DGLES2_IMPL_IMPLEMENTATION -DGLES2_SUPPORT_CLIENT_SIDE_ARRAYS=1 $
    -DGL_GLEXT_PROTOTYPES -D__STDC_CONSTANT_MACROS -D__STDC_FORMAT_MACROS $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../third_party/khronos -I../../gpu -I../.. -Igen/ui/gl $
    -I../../third_party/mesa/src/include
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wall -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics $
    -I/usr/local/include/gtk-2.0 -I/usr/local/include/gio-unix-2.0/ $
    -I/usr/local/include/atk-1.0 -I/usr/local/include/cairo $
    -I/usr/local/include/pixman-1 -D_THREAD_SAFE $
    -I/usr/local/include/gdk-pixbuf-2.0 -I/usr/local/include/libpng15 $
    -I/usr/local/include/pango-1.0 -I/usr/local/include/freetype2 $
    -I/usr/local/include/harfbuzz -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -pthread -I/usr/local/include/glib-2.0 $
    -I/usr/local/include -D_THREAD_SAFE -I/usr/local/include -O0 -g
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -Wsign-compare $
    -std=gnu++11

build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.buffer_tracker.o: $
    cxx ../../gpu/command_buffer/client/buffer_tracker.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.client_context_state.o: $
    cxx ../../gpu/command_buffer/client/client_context_state.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_implementation.o: $
    cxx ../../gpu/command_buffer/client/gles2_implementation.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_interface.o: $
    cxx ../../gpu/command_buffer/client/gles2_interface.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_trace_implementation.o: $
    cxx ../../gpu/command_buffer/client/gles2_trace_implementation.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gpu_memory_buffer_tracker.o: $
    cxx ../../gpu/command_buffer/client/gpu_memory_buffer_tracker.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.program_info_manager.o: $
    cxx ../../gpu/command_buffer/client/program_info_manager.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.query_tracker.o: $
    cxx ../../gpu/command_buffer/client/query_tracker.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.share_group.o: $
    cxx ../../gpu/command_buffer/client/share_group.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp
build $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.vertex_array_object_manager.o: $
    cxx ../../gpu/command_buffer/client/vertex_array_object_manager.cc || $
    obj/ui/gl/gl.actions_rules_copies.stamp

build obj/gpu/libgles2_implementation_client_side_arrays.a: alink_thin $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.buffer_tracker.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.client_context_state.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_implementation.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_interface.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gles2_trace_implementation.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.gpu_memory_buffer_tracker.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.program_info_manager.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.query_tracker.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.share_group.o $
    obj/gpu/command_buffer/client/gles2_implementation_client_side_arrays.vertex_array_object_manager.o $
    || obj/ui/gl/libgl_wrapper.a

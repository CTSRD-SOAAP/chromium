build obj/chrome/libcommon_constants.a: alink_thin $
    obj/chrome/obj/chrome/common_constants.gen/common_constants.chrome_version.o $
    obj/extensions/common/common_constants.constants.o $
    obj/chrome/common/common_constants.chrome_constants.o $
    obj/chrome/common/common_constants.chrome_paths.o $
    obj/chrome/common/common_constants.chrome_paths_linux.o $
    obj/chrome/common/common_constants.chrome_switches.o $
    obj/chrome/common/common_constants.env_vars.o $
    obj/chrome/common/net/common_constants.test_server_locations.o $
    obj/chrome/common/common_constants.pref_names.o $
    obj/chrome/common/common_constants.widevine_cdm_constants.o || $
    obj/chrome/common_constants.gen/chrome_version.cc

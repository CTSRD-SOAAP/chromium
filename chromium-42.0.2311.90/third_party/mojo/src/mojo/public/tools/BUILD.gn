import("../mojo.gni")

if (use_prebuilt_mojo_shell) {
  copy("copy_mojo_shell") {
    filename = "mojo_shell"
    if (is_win) {
      filename += ".exe"
    }
    if (is_android) {
      filename = "MojoShell.apk"
    }
    sources = [
      "prebuilt/$filename",
    ]
    if (is_android) {
      outputs = [
        "$root_out_dir/apks/$filename",
      ]
    } else {
      outputs = [
        "$root_out_dir/$filename",
      ]
    }
  }
}

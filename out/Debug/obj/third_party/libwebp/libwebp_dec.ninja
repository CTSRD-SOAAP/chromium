defines = -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../third_party/libwebp
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -std=gnu++11 -Wno-deprecated

build obj/third_party/libwebp/dec/libwebp_dec.alpha.o: cc $
    ../../third_party/libwebp/dec/alpha.c
build obj/third_party/libwebp/dec/libwebp_dec.buffer.o: cc $
    ../../third_party/libwebp/dec/buffer.c
build obj/third_party/libwebp/dec/libwebp_dec.frame.o: cc $
    ../../third_party/libwebp/dec/frame.c
build obj/third_party/libwebp/dec/libwebp_dec.idec.o: cc $
    ../../third_party/libwebp/dec/idec.c
build obj/third_party/libwebp/dec/libwebp_dec.io.o: cc $
    ../../third_party/libwebp/dec/io.c
build obj/third_party/libwebp/dec/libwebp_dec.layer.o: cc $
    ../../third_party/libwebp/dec/layer.c
build obj/third_party/libwebp/dec/libwebp_dec.quant.o: cc $
    ../../third_party/libwebp/dec/quant.c
build obj/third_party/libwebp/dec/libwebp_dec.tree.o: cc $
    ../../third_party/libwebp/dec/tree.c
build obj/third_party/libwebp/dec/libwebp_dec.vp8.o: cc $
    ../../third_party/libwebp/dec/vp8.c
build obj/third_party/libwebp/dec/libwebp_dec.vp8l.o: cc $
    ../../third_party/libwebp/dec/vp8l.c
build obj/third_party/libwebp/dec/libwebp_dec.webp.o: cc $
    ../../third_party/libwebp/dec/webp.c

build obj/third_party/libwebp/libwebp_dec.a: alink_thin $
    obj/third_party/libwebp/dec/libwebp_dec.alpha.o $
    obj/third_party/libwebp/dec/libwebp_dec.buffer.o $
    obj/third_party/libwebp/dec/libwebp_dec.frame.o $
    obj/third_party/libwebp/dec/libwebp_dec.idec.o $
    obj/third_party/libwebp/dec/libwebp_dec.io.o $
    obj/third_party/libwebp/dec/libwebp_dec.layer.o $
    obj/third_party/libwebp/dec/libwebp_dec.quant.o $
    obj/third_party/libwebp/dec/libwebp_dec.tree.o $
    obj/third_party/libwebp/dec/libwebp_dec.vp8.o $
    obj/third_party/libwebp/dec/libwebp_dec.vp8l.o $
    obj/third_party/libwebp/dec/libwebp_dec.webp.o

defines = -DANGLE_DX11 -DANGLE_DISABLE_TRACE $
    -DANGLE_COMPILE_OPTIMIZATION_LEVEL=D3DCOMPILE_OPTIMIZATION_LEVEL1 $
    '-DANGLE_PRELOADED_D3DCOMPILER_MODULE_NAMES={ $
    TEXT("d3dcompiler_46.dll"), TEXT("d3dcompiler_43.dll") }' $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 -DUSE_NSS=1 $
    -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN -DGTK_DISABLE_SINGLE_INCLUDES=1 $
    -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DUSE_PROPRIETARY_CODECS -DENABLE_CONFIGURATION_POLICY $
    -DENABLE_INPUT_SPEECH -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 -DNDEBUG $
    -DOFFICIAL_BUILD -DNVALGRIND -DDYNAMIC_ANNOTATIONS_ENABLED=0
includes = 
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics -Wno-format -O2 $
    -fdata-sections -ffunction-sections
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -std=gnu++11 -Wno-deprecated

build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DiagnosticsBase.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/DiagnosticsBase.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DirectiveHandlerBase.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/DirectiveHandlerBase.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DirectiveParser.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/DirectiveParser.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.ExpressionParser.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/ExpressionParser.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Input.o: $
    cxx ../../third_party/angle_dx11/src/compiler/preprocessor/Input.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Lexer.o: $
    cxx ../../third_party/angle_dx11/src/compiler/preprocessor/Lexer.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Macro.o: $
    cxx ../../third_party/angle_dx11/src/compiler/preprocessor/Macro.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.MacroExpander.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/MacroExpander.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Preprocessor.o: $
    cxx $
    ../../third_party/angle_dx11/src/compiler/preprocessor/Preprocessor.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Token.o: $
    cxx ../../third_party/angle_dx11/src/compiler/preprocessor/Token.cpp
build $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Tokenizer.o: $
    cxx ../../third_party/angle_dx11/src/compiler/preprocessor/Tokenizer.cpp

build obj/third_party/angle_dx11/src/libpreprocessor.a: alink_thin $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DiagnosticsBase.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DirectiveHandlerBase.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.DirectiveParser.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.ExpressionParser.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Input.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Lexer.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Macro.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.MacroExpander.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Preprocessor.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Token.o $
    obj/third_party/angle_dx11/src/compiler/preprocessor/preprocessor.Tokenizer.o

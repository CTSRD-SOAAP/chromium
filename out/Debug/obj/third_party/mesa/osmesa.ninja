build obj/third_party/mesa/osmesa.actions_depends.stamp: stamp $
    obj/third_party/mesa/libmesa.a $
    obj/third_party/mesa/libmesa_libglslcommon.a

defines = '-DMAPI_ABI_HEADER="glapi_mapi_tmp_shared.h"' $
    '-DPACKAGE_NAME="Mesa"' '-DPACKAGE_TARNAME="mesa"' $
    '-DPACKAGE_VERSION="9.0.3"' '-DPACKAGE_STRING="Mesa\ 9.0.3"' $
    '-DPACKAGE_BUGREPORT="https://bugs.freedesktop.org/enter_bug.cgi\?product=Mesa"' $
    '-DPACKAGE_URL=""' '-DPACKAGE="mesa"' '-DVERSION="9.0.3"' $
    -DSTDC_HEADERS=1 -DHAVE_SYS_TYPES_H=1 -DHAVE_SYS_STAT_H=1 $
    -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_MEMORY_H=1 $
    -DHAVE_STRINGS_H=1 -DHAVE_INTTYPES_H=1 -DHAVE_STDINT_H=1 $
    -DHAVE_DLFCN_H=1 '-DLT_OBJDIR=".libs/"' -DYYTEXT_POINTER=1 $
    -DHAVE_LIBEXPAT=1 -DHAVE_LIBXCB_DRI2=1 -DFEATURE_GL=1 -DMAPI_MODE_GLAPI $
    -DIN_DRI_DRIVER -DUSE_XCB -DGLX_INDIRECT_RENDERING $
    -DGLX_DIRECT_RENDERING -DUSE_EXTERNAL_DXTN_LIB=1 -DIN_DRI_DRIVER $
    -DHAVE_ALIAS -DHAVE_MINCORE -DHAVE_LIBUDEV -D_GLAPI_NO_EXPORTS $
    -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DHAVE_DLOPEN -DHAVE_PTHREAD=1 -DHAVE_UNISTD_H=1 -DHAVE_POSIX_MEMALIGN $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DUSE_XI2_MT=2 -DENABLE_REMOTING=1 $
    -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_EXTENSIONS=1 -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 -DCLD_VERSION=2 $
    -DENABLE_FULL_PRINTING=1 -DENABLE_PRINTING=1 -DENABLE_SPELLCHECK=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../third_party/mesa/src/src/mapi $
    -I../../third_party/mesa/src/src/mesa $
    -I../../third_party/mesa/src/src/mesa/drivers $
    -I../../third_party/mesa/src/chromium_gensrc/mesa $
    -I../../third_party/mesa/src/include
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -Wno-deprecated-register $
    -Wno-deprecated-declarations -fcolor-diagnostics -fPIC $
    -Wno-unknown-pragmas -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-stack-protector -Wno-unknown-warning-option $
    -fno-strict-aliasing -fno-rtti -fno-threadsafe-statics $
    -fvisibility-inlines-hidden -std=gnu++11 -Wno-deprecated

build $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o: $
    cc ../../third_party/mesa/src/src/mesa/drivers/common/driverfuncs.c
build obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/common/meta.c
build obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.c

ldflags = -Wl,-z,now -Wl,-z,relro -pthread -Wl,-z,noexecstack -fPIC $
    -Wl,--no-keep-memory
libs = 
build libosmesa.so libosmesa.so.TOC: solink_module $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o $
    obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o $
    obj/third_party/mesa/libmesa.a $
    obj/third_party/mesa/libmesa_libglslcommon.a
  soname = libosmesa.so
  lib = libosmesa.so

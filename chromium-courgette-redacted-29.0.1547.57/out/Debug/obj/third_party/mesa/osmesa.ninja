defines = -DANGLE_DX11 -DMAPI_GLAPI_CURRENT -DHAVE_VA_COPY -DHAVE_STRNLEN $
    -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL -DCHROMIUM_BUILD $
    -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 -DUSE_NSS=1 $
    -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN -DGTK_DISABLE_SINGLE_INCLUDES=1 $
    -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 -DENABLE_CONFIGURATION_POLICY $
    -DENABLE_INPUT_SPEECH -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 $
    -DENABLE_EGLIMAGE=1 -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../third_party/mesa/src/src/mapi $
    -I../../third_party/mesa/src/src/mesa $
    -I../../third_party/mesa/src/src/mesa/drivers $
    -I../../third_party/mesa/src/include
cflags = -fPIC -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -std=gnu++11 $
    -Wno-deprecated

build $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o: $
    cc ../../third_party/mesa/src/src/mesa/drivers/common/driverfuncs.c
build obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/common/meta.c
build obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o: cc $
    ../../third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.c

ldflags = -Wl,-z,now -Wl,-z,relro -pthread -Wl,-z,noexecstack -fPIC $
    -Wl,--no-keep-memory
libs = 
build libosmesa.so libosmesa.so.TOC: solink_module $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.driverfuncs.o $
    obj/third_party/mesa/src/src/mesa/drivers/common/osmesa.meta.o $
    obj/third_party/mesa/src/src/mesa/drivers/osmesa/osmesa.osmesa.o $
    obj/third_party/mesa/libmesa.a
  postbuilds = 
  soname = libosmesa.so
  lib = libosmesa.so

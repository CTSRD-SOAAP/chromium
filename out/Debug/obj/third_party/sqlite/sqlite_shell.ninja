build obj/third_party/sqlite/sqlite_shell.actions_depends.stamp: stamp $
    obj/third_party/icu/libicuuc.a obj/third_party/sqlite/libsqlite3.a $
    obj/third_party/icu/libicudata.a obj/third_party/icu/libicui18n.a

defines = -DSQLITE_CORE -DSQLITE_ENABLE_BROKEN_FTS2 -DSQLITE_ENABLE_FTS2 $
    -DSQLITE_ENABLE_FTS3 -DSQLITE_ENABLE_ICU $
    -DSQLITE_ENABLE_MEMORY_MANAGEMENT -DSQLITE_SECURE_DELETE $
    -DSQLITE_SEPERATE_CACHE_POOLS -DTHREADSAFE -D_HAS_EXCEPTIONS=0 $
    -DANGLE_DX11 -D_FILE_OFFSET_BITS=64 -DNO_TCMALLOC -DDISABLE_NACL $
    -DCHROMIUM_BUILD -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 $
    -DUSE_NSS=1 -DUSE_X11=1 -DENABLE_ONE_CLICK_SIGNIN $
    -DGTK_DISABLE_SINGLE_INCLUDES=1 -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 $
    -DENABLE_CONFIGURATION_POLICY -DENABLE_INPUT_SPEECH $
    -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 -DENABLE_EGLIMAGE=1 $
    -DENABLE_TASK_MANAGER=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DENABLE_MANAGED_USERS=1 $
    -DU_USING_ICU_NAMESPACE=0 -DU_STATIC_IMPLEMENTATION $
    -DDYNAMIC_ANNOTATIONS_ENABLED=1 -DWTF_USE_DYNAMIC_ANNOTATIONS=1 -D_DEBUG
includes = -I../../third_party/icu/public/common -I../../third_party/sqlite $
    -I../..
cflags = -fstack-protector --param=ssp-buffer-size=4  -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wheader-hygiene -Wno-char-subscripts -Wno-unused-function $
    -Wno-covered-switch-default -Wstring-conversion -Wno-c++11-narrowing $
    -Wno-reserved-user-defined-literal -fcolor-diagnostics -Wno-format -O0 -g
cflags_c = -O2 -pipe -fno-strict-aliasing
cflags_cc = -O2 -pipe -fno-strict-aliasing -fno-rtti $
    -fno-threadsafe-statics -fvisibility-inlines-hidden -std=gnu++11 $
    -Wno-deprecated

build obj/third_party/sqlite/src/src/sqlite_shell.shell.o: cc $
    ../../third_party/sqlite/src/src/shell.c
build obj/third_party/sqlite/src/src/sqlite_shell.shell_icu_linux.o: cc $
    ../../third_party/sqlite/src/src/shell_icu_linux.c

ldflags = -Wl,-z,now -Wl,-z,relro -pthread -Wl,-z,noexecstack -fPIC $
    -Wl,--no-keep-memory
libs = 
build sqlite_shell: link $
    obj/third_party/sqlite/src/src/sqlite_shell.shell.o $
    obj/third_party/sqlite/src/src/sqlite_shell.shell_icu_linux.o $
    obj/third_party/sqlite/libsqlite3.a obj/third_party/icu/libicuuc.a $
    obj/third_party/icu/libicudata.a obj/third_party/icu/libicui18n.a
  postbuilds = 

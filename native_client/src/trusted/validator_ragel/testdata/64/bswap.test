@hex:
  # Test handling of different sizes for instruction bswap.
  # 32-bit example
  0f c8
  # 64-bit example
  48 0f c8
  # Bad case, we should not parse 16-bit operands
  66 0f c8
  
@dis:
     0:	0f c8                                        	bswap  %eax
     2:	48 0f c8                                     	bswap  %rax
     5:	66 0f c8                                     	bswap  %ax
@rdfa_output:
  5: [0] unrecognized instruction
  return code: 1
